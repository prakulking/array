#include<stdio.h>
#include<conio.h>

int main()
{
	 char ch;
	 clrscr();
	 printf("Enter any character: ");
	 scanf("%c", &ch);
	
	 /* Converting to lowercase if it is uppercase. */
	 ch = ch>='A'&&ch<='Z'?ch+32:ch;
	
	 /* Making Decision */
	 if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
	 {
	  	printf("It is VOWEL.");
	 }
	 else
	 {
	  	printf("It is NOT VOWEL.");
	 }
	
	 getch();
	 return(0);
}
